{
  "name": "githelper",
  "displayName": "Git Helper",
  "description": "A VS Code extension that fetches Git logs, analyzes them with an LLM, and provides a chatbot interface.",
  "version": "0.1.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Xianglinn/githelper"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:githelper.sendGitLogToLLM",
    "onCommand:githelper.openChatbot"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "githelper.gitView",
          "name": "GitHelper Logs & Chat"
        }
      ]
    },
    "commands": [
      {
        "command": "githelper.fetchGitLog",
        "title": "Fetch Git Log"
      },
      {
        "command": "githelper.clearLogs",
        "title": "Clear Logs"
      },
      {
        "command": "githelper.openChat",
        "title": "Open Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.2.3",
    "chai": "^5.2.0",
    "mocha": "^11.1.0",
    "typescript": "^5.8.2",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^1.14.0",
    "@ibm-cloud/watsonx-ai": "^1.6.0",
    "@langchain/community": "^0.3.36",
    "@langchain/core": "^0.3.40",
    "@langchain/ollama": "^0.2.0",
    "@playwright/test": "^1.42.1",
    "axios": "^1.7.8",
    "deepmerge": "^4.3.1",
    "ibm-cloud-sdk-core": "^5.3.2",
    "openai": "^4.7.1"
  }
}
